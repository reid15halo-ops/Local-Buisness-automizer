            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "confidence-check",
              "leftValue": "={{ $json[\"confidence\"] }}",
              "rightValue": 0.95,
              "operator": {
                "type": "number",
                "operation": "gte"
