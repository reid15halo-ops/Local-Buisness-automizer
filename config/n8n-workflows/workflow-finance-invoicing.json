          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ JSON.stringify(JSON.parse($json[\"choices\"][0][\"message\"][\"content\"])) }}",
        "options": {
          "timeout": 10000
        },
        "continueOnFail": true
      },
      "id": "node-math-validate",
      "name": "Math Validate (Zone 2)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1340, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
